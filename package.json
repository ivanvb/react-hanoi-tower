{
    "name": "hanoi-dnd-react",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "concurrently \"yarn build:tailwind --watch\" \"vite --host\"",
        "build:tailwind": "tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/tailwind.build.css",
        "build": "yarn build:tailwind && vite build",
        "preview": "yarn vite preview"
    },
    "dependencies": {
        "react": "^17.0.2",
        "react-beautiful-dnd": "^13.1.0",
        "react-dom": "^17.0.2",
        "reakit": "^1.3.11"
    },
    "devDependencies": {
        "@vitejs/plugin-react": "^1.0.7",
        "concurrently": "^7.0.0",
        "tailwindcss": "^3.0.23",
        "vite": "^2.8.0"
    }
}
